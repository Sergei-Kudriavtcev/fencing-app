<ClubCsvFormats xmlns="http://www.fencingtime.com" version="4.4">
  <CsvFormat authority="USFA" useSemicolons="False">
    <Description>A club list exported from askfred.net</Description>
    <ColumnList>
      <Column fieldType="ClubName"   	 colIndex="0" header="club_name" />
      <Column fieldType="ClubAbbr"     colIndex="1" header="abbreviation" />
      <Column fieldType="Division"   	 colIndex="2" header="division" />
      <Column fieldType="Country"      colIndex="3" header="country" />
      <Column fieldType="ClubLocation" colIndex="5" header="city" />
    </ColumnList>  
  </CsvFormat>

  <CsvFormat authority="CFF" useSemicolons="False">
    <Description>A club list downloaded from the CFF website</Description>
    <ColumnList>
      <Column fieldType="Division"   	  colIndex="0" header="Branch" />
      <Column fieldType="ClubAbbr"      colIndex="1" header="Club" />
      <Column fieldType="ClubName"   	  colIndex="2" header="Club Name" />
      <Column fieldType="ClubMemberNum" colIndex="1" header="Club" />
    </ColumnList>  
    <PreProcess operation="1" />
  </CsvFormat>
</ClubCsvFormats>
