<RefereeCsvFormats xmlns="http://www.fencingtime.com" version="4.4">
  <CsvFormat authority="USFA" useSemicolons="False">
    <Description>A referee list exported from foc.askfred.net</Description>
    <ColumnList>
      <Column fieldType="LastName"   	    colIndex="0" header="Last Name" />
      <Column fieldType="FirstName"       colIndex="1" header="First Name" />
      <Column fieldType="Division"   	    colIndex="2" header="Division" />
      <Column fieldType="FoilRefRating"   colIndex="5" header="USFA Foil" />
      <Column fieldType="EpeeRefRating"   colIndex="7" header="USFA Epee" />
      <Column fieldType="SaberRefRating"  colIndex="9" header="USFA Saber" />
    </ColumnList>  
  </CsvFormat>
  
  <CsvFormat authority="USFA" useSemicolons="False">
    <Description>A USFA hired referee list</Description>
  	<ColumnList>
      <Column fieldType="LastName"   	    colIndex="0" header="Last Name" />
      <Column fieldType="FirstName"       colIndex="1" header="First Name" />
      <Column fieldType="MemberNum"       colIndex="2" header="MemberNum" />
      <Column fieldType="Division"   	    colIndex="3" header="Division" />
      <Column fieldType="FoilRefRating"   colIndex="4" header="Foil" />
      <Column fieldType="EpeeRefRating"   colIndex="5" header="Epee" />
      <Column fieldType="SaberRefRating"  colIndex="6" header="Saber" />
    </ColumnList>  
  </CsvFormat>
</RefereeCsvFormats>
