<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:usfa="https://member.usafencing.org/" xmlns="https://member.usafencing.org/" targetNamespace="https://member.usafencing.org/" elementFormDefault="qualified">
  <xs:annotation>
    <xs:documentation>4.4</xs:documentation>
  </xs:annotation>

  <xs:element name="FencingData">
    <xs:complexType>
      <xs:all>
        <xs:element name="ClubDatabase"    type="usfa:ClubDatabaseType"   minOccurs="1" maxOccurs="1" />
        <xs:element name="PersonDatabase"  type="usfa:PersonDatabaseType" minOccurs="1" maxOccurs="1" /> 
        <xs:element name="Tournament"      type="TournamentType"          minOccurs="1" maxOccurs="1" />
        <xs:element name="RegNoteDatabase" type="RegNoteDatabaseType"     minOccurs="0" maxOccurs="1" /> 
      </xs:all>
      
      <xs:attribute name="version" type="xs:string" fixed="1.0" />
    </xs:complexType>

    <xs:unique name="clubKey">
      <xs:selector xpath="usfa:ClubDatabase/usfa:Club" />
      <xs:field    xpath="@USFAMemberNum" />
    </xs:unique>
    <xs:unique name="personKey">
      <xs:selector xpath="usfa:PersonDatabase/usfa:Person" />
      <xs:field    xpath="@PersonID"   />
    </xs:unique>
    
    <xs:keyref name="personClubKeyConstraint1" refer="usfa:clubKey">
      <xs:selector xpath="usfa:PersonDatabase/usfa:Person" />
      <xs:field    xpath="@Club1" />
    </xs:keyref>        
    <xs:keyref name="personClubKeyConstraint2" refer="usfa:clubKey">
      <xs:selector xpath="usfa:PersonDatabase/usfa:Person" />
      <xs:field    xpath="@Club2" />
    </xs:keyref>        
    <xs:keyref name="regPersonKeyConstraint" refer="usfa:personKey">
      <xs:selector xpath="usfa:Tournament/usfa:Events/usfa:Event/usfa:Reg" />
      <xs:field    xpath="@PersonID" />
    </xs:keyref>        
    <xs:keyref name="teamFencerPersonKeyConstraint" refer="usfa:personKey">
      <xs:selector xpath="usfa:Tournament/usfa:Events/usfa:Event/usfa:TeamReg/usfa:TeamFencer" />
      <xs:field    xpath="@PersonID" />
    </xs:keyref>        
    <xs:keyref name="refereePersonKeyConstraint" refer="usfa:personKey">
      <xs:selector xpath="usfa:Tournament/usfa:Referees/usfa:Referee" />
      <xs:field    xpath="@PersonID" />
    </xs:keyref>        
    <xs:keyref name="regNotePersonKeyConstraint" refer="usfa:personKey">
      <xs:selector xpath="usfa:RegNoteDatabase/usfa:Note" />
      <xs:field    xpath="@PersonID" />
    </xs:keyref>        
  </xs:element>

  <xs:complexType name="ClubDatabaseType">
    <xs:sequence>
      <xs:element name="Club" minOccurs="0" maxOccurs="unbounded">
        <xs:complexType>
          <xs:attribute name="USFAMemberNum" type="usfa:MemberNumType"    use="required" />
          <xs:attribute name="Name"          type="usfa:NonEmptyString"   use="required" />
          <xs:attribute name="Abbreviation"  type="usfa:NonEmptyString"   use="optional" />
          <xs:attribute name="Location"      type="xs:string"             use="optional" />
          <xs:attribute name="Division"      type="usfa:DivisionType"     use="optional" />
          <xs:attribute name="Country"       type="usfa:CountryType"      use="optional" />
          <xs:attribute name="Expires"       type="usfa:DateType"         use="optional" />
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  
  <xs:complexType name="PersonDatabaseType">
    <xs:sequence>
      <xs:element name="Person" minOccurs="0" maxOccurs="unbounded">
        <xs:complexType>
          <!-- Memberships -->
          <xs:all>
            <xs:element name="Memberships" minOccurs="1" maxOccurs="1">
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="Membership" minOccurs="1" maxOccurs="unbounded">
                    <xs:complexType>
                      <xs:simpleContent>
                        <xs:extension base="usfa:MemberNumType">
                          <xs:attribute name="Org"                           type="usfa:NonEmptyString" use="required" />
                          <xs:attribute name="Expires"                       type="usfa:DateType"       use="required" />
                          <xs:attribute name="IsCompetitive"                 type="usfa:BoolType"       use="required" />
                          <xs:attribute name="SafeSportExpirationDate"       type="usfa:DateType"       use="optional" />
                          <xs:attribute name="BackgroundCheckExpirationDate" type="usfa:DateType"       use="optional" />
                        </xs:extension>
                      </xs:simpleContent>
                    </xs:complexType>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
            
            <!-- Weapon classifications -->
            <xs:element name="WeaponClassifications" minOccurs="0" maxOccurs="1">
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="Classification" minOccurs="0" maxOccurs="unbounded">
                    <xs:complexType>
                      <xs:simpleContent>
                        <xs:extension base="usfa:ClassificationType">
                          <xs:attribute name="Org"    type="usfa:NonEmptyString" use="required" />
                          <xs:attribute name="Weapon" type="usfa:WeaponType"     use="required" />
                        </xs:extension>
                      </xs:simpleContent>
                    </xs:complexType>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
            
            <!-- Referee ratings -->
            <xs:element name="RefereeRatings" minOccurs="0" maxOccurs="1">
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="Rating" minOccurs="0" maxOccurs="unbounded">
                    <xs:complexType>
                      <xs:simpleContent>
                        <xs:extension base="usfa:RefRatingType">
                          <xs:attribute name="Org"    type="usfa:NonEmptyString" use="required" />
                          <xs:attribute name="Weapon" type="usfa:WeaponType"     use="required" />
                        </xs:extension>
                      </xs:simpleContent>
                    </xs:complexType>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>

            <!-- Weapon rankings -->
            <xs:element name="WeaponRankings" minOccurs="0" maxOccurs="1">
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="Ranking" minOccurs="0" maxOccurs="unbounded">
                    <xs:complexType>
                      <xs:simpleContent>
                        <xs:extension base="xs:unsignedInt">
                          <xs:attribute name="Org"    type="usfa:NonEmptyString" use="required" />
                          <xs:attribute name="Weapon" type="usfa:WeaponType"     use="required" />
                          <xs:attribute name="List"   type="PointListType"       use="required" />
                        </xs:extension>
                      </xs:simpleContent>
                    </xs:complexType>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:all>
        
          <xs:attribute name="PersonID"            type="usfa:MemberNumType"    use="required" />
          <xs:attribute name="FirstName"           type="usfa:NonEmptyString"   use="required" />
          <xs:attribute name="LastName"            type="usfa:NonEmptyString"   use="required" />
          <xs:attribute name="MiddleName"          type="usfa:NonEmptyString"   use="optional" />
          <xs:attribute name="NickName"            type="usfa:NonEmptyString"   use="optional" />
          <xs:attribute name="Suffix"              type="usfa:NonEmptyString"   use="optional" />
          <xs:attribute name="Gender"              type="usfa:PersonGenderType" use="required" />
          <xs:attribute name="BirthYear"           type="usfa:YearType"         use="required" />
          <xs:attribute name="BirthdateVerified"   type="usfa:BoolType"         use="required" />
          <xs:attribute name="Handedness"          type="usfa:HandType"         use="optional" />
          <xs:attribute name="Division"            type="usfa:DivisionType"     use="optional" />
          <xs:attribute name="Country"             type="usfa:CountryType"      use="optional" />
          <xs:attribute name="CitizenshipVerified" type="usfa:BoolType"         use="required" />
          <xs:attribute name="RegionID"            type="usfa:RegionType"       use="optional" />
          <xs:attribute name="ProfilePicture"      type="usfa:NonEmptyString"   use="optional" />
          <xs:attribute name="RegistrationFeePaid" type="xs:decimal"            use="optional" />
          <xs:attribute name="Club1"               type="usfa:MemberNumType"    use="optional" />
          <xs:attribute name="Club2"               type="usfa:MemberNumType"    use="optional" />
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  
  <xs:complexType name="TournamentType">
    <xs:sequence>
      <xs:element name="Events" minOccurs="1" maxOccurs="1">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="Event" type="usfa:EventType" minOccurs="0" maxOccurs="unbounded">
                <xs:unique name="regPerson">
                  <xs:selector xpath="usfa:Reg" />
                  <xs:field    xpath="@PersonID"   />
                </xs:unique>
                <xs:unique name="teamRegPerson">
                  <xs:selector xpath="usfa:TeamReg/usfa:TeamFencer" />
                  <xs:field    xpath="@PersonID"   />
                </xs:unique>
            </xs:element>            
          </xs:sequence>
        </xs:complexType>
      </xs:element>

      <xs:element name="Referees" minOccurs="0" maxOccurs="1">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="Referee" minOccurs="0" maxOccurs="unbounded">
              <xs:complexType>
                <xs:attribute name="PersonID" type="usfa:MemberNumType"  use="required" />
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  
    <xs:attribute name="TournamentID"  type="xs:unsignedLong"    use="required" />
    <xs:attribute name="Name"          type="NonEmptyString"     use="required" />
    <xs:attribute name="Location"      type="NonEmptyString"     use="optional" />
    <xs:attribute name="StartDate"     type="usfa:DateType"      use="required" />
    <xs:attribute name="EndDate"       type="usfa:DateType"      use="required" />
    <xs:attribute name="City"          type="NonEmptyString"     use="optional" />
    <xs:attribute name="State"         type="StateType"          use="optional" />
    <xs:attribute name="Zip"           type="NonEmptyString"     use="optional" />
  </xs:complexType>
  
  <xs:complexType name="EventType">
    <xs:choice>
      <xs:element name="Reg" minOccurs="0" maxOccurs="unbounded">
        <xs:complexType>
          <xs:attribute name="PersonID"     type="usfa:MemberNumType"  use="required" />
          <xs:attribute name="EventFeePaid" type="xs:decimal"          use="optional" />
        </xs:complexType>
      </xs:element>

      <xs:element name="TeamReg" minOccurs="0" maxOccurs="unbounded">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="TeamFencer" minOccurs="0" maxOccurs="4">
              <xs:complexType>
                <xs:attribute name="PersonID" type="usfa:MemberNumType" use="required" />
              </xs:complexType>
            </xs:element>
          </xs:sequence>

          <xs:attribute name="TeamID"       type="xs:unsignedLong"    use="required" />
          <xs:attribute name="TeamName"     type="NonEmptyString"     use="required" />
          <xs:attribute name="TeamCaptain"  type="NonEmptyString"     use="optional" />
          <xs:attribute name="TeamClub"     type="usfa:MemberNumType" use="optional" />
          <xs:attribute name="TeamDivision" type="usfa:DivisionType"  use="optional" />
          <xs:attribute name="TeamCountry"  type="usfa:CountryType"   use="optional" />
          <xs:attribute name="EventFeePaid" type="xs:decimal"         use="optional" />
        </xs:complexType>
      </xs:element>
    </xs:choice>

    <xs:attribute name="EventID"         type="xs:unsignedLong"      use="required" />
    <xs:attribute name="EventDateTime"   type="usfa:DateTimeType"    use="required" />
    <xs:attribute name="Weapon"          type="usfa:WeaponType"      use="required" />
    <xs:attribute name="Gender"          type="usfa:EventGenderType" use="required" />
    <xs:attribute name="AgeLimit"        type="usfa:AgeLimitType"    use="required" />
    <xs:attribute name="RegionID"        type="usfa:RegionType"      use="required" />
    <xs:attribute name="IsTeam"          type="usfa:BoolType"        use="required" />
    <xs:attribute name="IsWheelchair"    type="usfa:BoolType"        use="required" />
    <xs:attribute name="RatingLimit"     type="usfa:RatingLimitType" use="required" />
    <xs:attribute name="EventScope"      type="usfa:EventScopeType"  use="required" />
    <xs:attribute name="EventFee"        type="xs:decimal"           use="optional" />
    <xs:attribute name="RegistrationFee" type="xs:decimal"           use="optional" />
  </xs:complexType>
   
  <xs:complexType name="RegNoteDatabaseType">
    <xs:sequence>
      <xs:element name="Note" minOccurs="0" maxOccurs="unbounded">
        <xs:complexType>
          <xs:attribute name="PersonID" type="usfa:MemberNumType"  use="required" />
          <xs:attribute name="Note"     type="usfa:NonEmptyString" use="required" />
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  
  <!-- Simple type definitions -->
  <xs:simpleType name="NonEmptyString">
    <xs:restriction base="xs:string">
      <xs:minLength value="1" />
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType name="DateType">
    <xs:restriction base="xs:string">
      <xs:pattern value="[1-2][0-9][0-9][0-9]-[0-1][0-9]-[0-3][0-9]" />
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType name="DateTimeType">
    <xs:restriction base="xs:string">
      <xs:pattern value="[1-2][0-9][0-9][0-9]-[0-1][0-9]-[0-3][0-9]T[0-2][0-9]:[0-5][0-9]:[0-5][0-9]" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="BoolType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="True"  />
      <xs:enumeration value="False" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="MemberNumType">
    <xs:restriction base="xs:string">
      <xs:pattern value="[1-9][0-9][0-9][0-9][0-9][0-9][0-9][0-9][0-9]" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="YearType">
    <xs:restriction base="xs:string">
      <xs:pattern value="[1-2][0-9][0-9][0-9]" />
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType name="ClassificationType">
    <xs:restriction base="xs:string">
      <xs:pattern value="U"               />
      <xs:pattern value="[A-E][0-9][0-9]" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="RefRatingType">
    <xs:restriction base="xs:string">
      <xs:pattern value="P"     />
      <xs:pattern value="10"    />
      <xs:pattern value="[1-9]" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="PersonGenderType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="M" />
      <xs:enumeration value="F" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="EventGenderType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Men"   />
      <xs:enumeration value="Women" />
      <xs:enumeration value="Mixed" />
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType name="HandType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="L" />
      <xs:enumeration value="R" />
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType name="RegionType">
    <xs:restriction base="xs:integer">
      <xs:minInclusive value="0" />
      <xs:maxInclusive value="6" />
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType name="WeaponType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Foil"  />
      <xs:enumeration value="Epee"  />
      <xs:enumeration value="Saber" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="PointListType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="FIE" />
      <xs:enumeration value="SR"  />
      <xs:enumeration value="VET" />
      <xs:enumeration value="V40" />
      <xs:enumeration value="V50" />
      <xs:enumeration value="V60" />
      <xs:enumeration value="V70" />
      <xs:enumeration value="V80" />
      <xs:enumeration value="JNR" />
      <xs:enumeration value="CDT" />
      <xs:enumeration value="Y14" />
      <xs:enumeration value="Y12" />
      <xs:enumeration value="Y10" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="AgeLimitType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Senior"      />
      <xs:enumeration value="Junior"      />
      <xs:enumeration value="Cadet"       />
      <xs:enumeration value="U19"         />
      <xs:enumeration value="U16"         />
      <xs:enumeration value="Y14"         />
      <xs:enumeration value="Y12"         />
      <xs:enumeration value="Y10"         />
      <xs:enumeration value="Y8"          />
      <xs:enumeration value="VetCombined" />
      <xs:enumeration value="Vet40"       />
      <xs:enumeration value="Vet50"       />
      <xs:enumeration value="Vet60"       />
      <xs:enumeration value="Vet70"       />
      <xs:enumeration value="Vet80"       />
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType name="RatingLimitType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Open"      />
      <xs:enumeration value="Unrated"   />
      <xs:enumeration value="EAndUnder" />
      <xs:enumeration value="DivIII"    />
      <xs:enumeration value="DivII"     />
      <xs:enumeration value="DivI"      />
      <xs:enumeration value="DivIA"     />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="EventScopeType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="RYC"     />
      <xs:enumeration value="SYC"     />
